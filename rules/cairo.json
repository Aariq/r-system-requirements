{
  "patterns": ["\\bcairo\\b"],
  "dependencies": [
    {
      "packages": ["libcairo2-dev"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "ubuntu",
          "version": [
            {
              "operator":  ">=",
              "value": "xenial"
            },
            {
              "operator":  "<=",
              "value": "bionic"
            }
          ]
        },
        {
          "os": "linux",
          "distribution": "debian"
        }
      ]
    },
    {
      "packages": ["cairo-devel"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "centos",
          "version": [
            {
              "operator": "!=",
              "value": "6"
            }
          ]
        },
        {
          "os": "linux",
          "distribution": "redhat",
          "version": [
            {
              "value": "6"
            }
          ]
        }
      ]
    },
    {
      "packages": ["cairo-devel"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "opensuse"
        },
        {
          "os": "linux",
          "distribution": "sle"
        }
      ]
    }
  ]
}